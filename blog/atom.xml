---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Black Radley News</title>
	<link href="https://www.blackradley.com/blog/atom.xml" rel="self"/>
	<link href="https://www.blackradley.com/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://www.blackradley.com/blog</id>
	<author>
		<name>Joe Collins</name>
		<email>general@blackradley.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://www.blackradley.com{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://www.blackradley.com{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>